{
    "require": ["std:get"],
    "specs": [
        {
            "title": "Get key in object",
            "input": ["ok", {"foo": "FOO", "bar": "BAR"}],
            "flow": [["ok", "std:get", "bar"]],
            "expected": ["ok", "BAR"]
        },
        {
            "title": "Get value from array by index",
            "input": ["ok", ["FOO", "BAR"]],
            "flow": [["ok", "std:get", 1]],
            "expected": ["ok", "BAR"]
        },
        {
            "title": "Get char from string by index",
            "input": ["ok", "BAR"],
            "flow": [["ok", "std:get", 1]],
            "expected": ["ok", "A"]
        },
        {
            "title": "Error on wrong type",
            "input": ["ok", null],
            "flow": [["ok", "std:get", 1]],
            "expected": ["error", ["Function is not implemented for input type", "If is not implemented"]]
        }
